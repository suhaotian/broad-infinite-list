{
  "name": "broad-infinite-list",
  "version": "1.3.0",
  "description": "The high performance and bidirectional scrolling infinite list component for React, Vue 3 and React Native",
  "repository": "suhaotian/broad-infinite-list",
  "bugs": "https://github.com/suhaotian/broad-infinite-list/issues",
  "homepage": "https://suhaotian.github.io/broad-infinite-list",
  "type": "module",
  "main": "./dist/react.js",
  "react-native": "./dist/react-native.js",
  "types": "./dist/react.d.ts",
  "exports": {
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./react-native": {
      "types": "./dist/react-native.d.ts",
      "react-native": "./dist/react-native.js",
      "require": "./dist/cjs/react-native.js",
      "import": "./dist/react-native.js"
    },
    "./vue": {
      "import": "./dist/vue.js",
      "types": "./dist/index.vue.d.ts"
    }
  },
  "workspaces": [
    "demos/**",
    "vue-example/**",
    "./**"
  ],
  "dependencies": {
    "use-next-tick": "^1.1.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue": "^3.5.27",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "vue": ">=3",
    "react-native": ">=0.80"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "scripts": {
    "build": "rm -rf dist && bun run build:vue && bun build.ts && bun build-rn.ts&& bun build-rn-cjs.ts && tsc --emitDeclarationOnly",
    "build:vue": "vite build && vue-tsc --declaration --emitDeclarationOnly"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "LICENSE",
    "README.md",
    "flow.svg"
  ],
  "keywords": [
    "react",
    "infinite loading",
    "infinite list",
    "infinite scroll",
    "bidirectional",
    "bidirectional scroll list",
    "large list",
    "huge list",
    "performance",
    "react-native",
    "expo",
    "vue",
    "vue3"
  ],
  "author": {
    "name": "@suhaotian",
    "url": "https://github.com/suhaotian"
  },
  "license": "MIT"
}
